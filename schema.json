{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "@runnerty-executor-mysql",
  "definitions": {
    "config": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "type": {
          "type": "string",
          "pattern": "@runnerty-executor-mysql"
        },
        "user": {
          "type": "string"
        },
        "password": {
          "type": "string"
        },
        "crypted_password": {
          "type": "string"
        },
        "database": {
          "type": "string"
        },
        "host": {
          "type": "string"
        },
        "port": {
          "type": "string"
        },
        "socketPath": {
          "type": "string"
        },
        "charset": {
          "type": "string"
        },
        "timezone": {
          "type": "string"
        },
        "insecureAuth": {
          "type": "boolean"
        },
        "debug": {
          "type": "boolean"
        },
        "flags": {
          "type": "string"
        },
        "multipleStatements": {
          "type": "boolean"
        },
        "ssl": {
          "type": "object"
        },
        "connectTimeout": {
          "type": "number"
        },
        "csvFileExport": {
          "type": "string"
        },
        "xlsxFileExport": {
          "type": "string"
        },
        "xlsxAuthorName": {
          "type": "string"
        },
        "xlsxSheetName": {
          "type": "string"
        }
      }
    },
    "params": {
      "oneOf": [
        {
          "type": "object",
          "required": [
            "id",
            "command"
          ],
          "properties": {
            "id": {
              "type": "string"
            },
            "command": {
              "type": "string"
            },
            "args": {
              "type": "object"
            },
            "csvFileExport": {
              "type": "string"
            },
            "xlsxFileExport": {
              "type": "string"
            },
            "xlsxAuthorName": {
              "type": "string"
            },
            "xlsxSheetName": {
              "type": "string"
            }
          }
        },
        {
          "type": "object",
          "required": [
            "id",
            "command_file"
          ],
          "properties": {
            "id": {
              "type": "string"
            },
            "command_file": {
              "type": "string"
            },
            "args": {
              "type": "object"
            },
            "csvFileExport": {
              "type": "string"
            },
            "xlsxFileExport": {
              "type": "string"
            },
            "xlsxAuthorName": {
              "type": "string"
            },
            "xlsxSheetName": {
              "type": "string"
            }
          }
        }
      ]
    }
  }
}